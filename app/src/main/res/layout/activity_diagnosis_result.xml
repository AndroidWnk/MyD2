        <RelativeLayout style="@style/diagnosis_result_line_style">

            <TextView
                android:id="@+id/tv_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:textStyle="bold"
                android:text="您的车辆健康得分(共检查6项):"
                android:textColor="@color/white"
                android:textSize="26sp" />

            <TextView
                android:id="@+id/tv_result_mark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/tv_result"
                android:layout_marginLeft="30dp"
                android:textStyle="bold"
                android:text="100分"
                android:textColor="@color/roundpg_text"
                android:textSize="28sp" />
        </RelativeLayout>

        <RelativeLayout style="@style/diagnosis_line_style"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >

            <TextView
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="检查项"
                android:textColor="@color/white"
                android:textSize="22sp" />

            <TextView
                android:id="@+id/tv_jieguo"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="结果"
                android:textColor="@color/white"
                android:textSize="22sp" />
            <TextView
                android:layout_weight="1"
                android:id="@+id/tv_bigLightTest2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="详情"
                android:textColor="@color/white"
                android:textSize="22sp" />
            </LinearLayout>


        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@drawable/tiao" />

        <RelativeLayout style="@style/diagnosis_result_line_style"
            >

