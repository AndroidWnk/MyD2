<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" >
         Customize your theme here.
     <!--   <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>-->
    </style>

    <style name="MainActivtiyButtonStyle">
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MainActivtiyButtonIconStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginBottom">3px</item>
    </style>
    <style name="MainActivtiyButtonTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="MainActivtiyButtonCountTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">15px</item>
        <item name="android:textColor">#A6A6A6</item>
    </style>

    <style name="diagnosis_result_line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">69dp</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="diagnosis_line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/title_bg_d2s</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="imformation_line_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">28sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:gravity">center_vertical</item>
    </style>

</resources>
